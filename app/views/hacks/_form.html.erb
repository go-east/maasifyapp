<%= simple_form_for(@hack) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :titre %>
    <%= f.input :description %>
    <%= f.input :categorie_une %>
    <%= f.input :qualite %>
    <%= f.input :image %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
